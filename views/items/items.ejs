
<nav class="background-grey boarder">
    <a class="nav-margin" href="/champs">List of Champs</a>
    <a class="nav-margin" href="/summoner">Search for Summoner</a>
    <a class="nav-margin" href="/profile">Back to  Profile</a>
</nav>

<p class="title centered">
    List of LOL Items
</p>

<div class="item-container">

<% name.forEach((el) =>{ %>

    <div class="items">
        <ul>
            <li><a href="/items/<%= el %>"><%= itemsObj[el].name %></a></li>
            <img class ="itemImg" src="https://ddragon.leagueoflegends.com/cdn/11.23.1/img/item/<%= el %>.png" alt="image of <%= itemsObj[el].name %>">
        </ul>
    </div>

<% }) %>

</div>